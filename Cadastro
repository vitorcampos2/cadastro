   <script> 
       var nomedocandidato = document.getElementById("nomeUsuario").value;
            console.log(nome);
            var datadenascimento=document.getElementById("datadenascimento").value;
            var email=document.getElementById("email").value;
            function validar(){
                var nomedocandidato= document.getElementById("nomeUsuario").value;
                var datadenascimento= document.getElementById("datadenascimento").value;
                var email= document.getElementById("email").value;
                var cargo= document.getElementById("cargo").value;
              
                var arr=[];
                arr.push(nomedocandidato);
                arr.push(datadenascimento);
                arr.push(email);
                arr.push(cargo);
             
                for(var i= 0; i < arr.length;i++){
                    if(arr[i]==false){
                        alert("campo obrigatorio vazio");
                        return;
                    }
                }
            }
      
    </script>
